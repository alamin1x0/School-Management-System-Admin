<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admindashboard">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/admin"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/admin_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminDashboard">
        <activity
            android:name=".activity.pdf.OldPdfPrintActivity"
            android:exported="false" />
        <activity
            android:name=".activity.pdf.InvoiceActivity"
            android:exported="false" />
        <activity
            android:name=".activity.result.UploadResult"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.result.UpdateResult"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.result.AddResult"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.otp.OtpVerifyActivity" />
        <activity android:name=".activity.otp.OtpProfileAactivity" />
        <activity android:name=".activity.otp.OtpMainActivity" />
        <activity android:name=".activity.otp.OtpActivity" />
        <activity
            android:name=".activity.student.UpdateStudent"
            android:parentActivityName=".activity.student.Uploadstudent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.student.AddStudent"
            android:parentActivityName=".activity.student.Uploadstudent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.student.Uploadstudent"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.login.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.notice.DeleteNoticeActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.teacher.AddTeacher"
            android:parentActivityName=".activity.teacher.Uploadfaculty"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.teacher.UpdateTeacherActivity"
            android:parentActivityName=".activity.teacher.Uploadfaculty"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.teacher.Uploadfaculty"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ebook.UploadPdfActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.image.UploadImage"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.notice.UploadNotice"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>